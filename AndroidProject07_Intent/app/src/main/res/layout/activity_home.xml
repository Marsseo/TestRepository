<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mycompany.myapplication.activity.HomeActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleBtnUIActivity"
        android:text="UIActivity로 이동"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="74dp" />

    <Button
        android:id="@+id/btnWebBrowser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleBtnWebBrower"
        android:text="WebBrower로 이동"
        tools:textAllCaps="false" />

    <Button
        android:id="@+id/btnDialActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleBtnDialActivity"
        android:text="DialActivity로 이동" />

    <Button
        android:id="@+id/btnMap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleBtnMapActivity"
        android:text="지도 보기" />

    <Button
        android:id="@+id/btnDataSend"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleBtnDataSend"
        android:text="데이터 전달" />

    <Button
        android:id="@+id/btnReturnValue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleBtnReturnValue"
        android:text="리턴값 받기" />

    <Button
        android:id="@+id/btnFileSelect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleFileSelect"
        android:text="파일선택" />
</LinearLayout>
